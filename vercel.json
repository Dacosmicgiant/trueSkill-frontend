{
  "version": 2,
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://skillshub-api.onrender.com/api/$1"
    },
    { 
      "handle": "filesystem"
    },
    { 
      "src": "/(.*)", 
      "dest": "/index.html" 
    }
  ]
}