{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && npm install",
  "framework": null,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://skillshub-api.onrender.com/api/$1"
    },
    { 
      "handle": "filesystem"
    },
    { 
      "src": "/(.*)", 
      "dest": "/frontend/dist/index.html" 
    }
  ]
}